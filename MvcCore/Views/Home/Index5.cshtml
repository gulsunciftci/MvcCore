
@model List<MvcCore.Models.Kitap>
@{
    Layout=null;
}


<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Index5</title>
    </head>
    <body>
       <h2>Merhaba Kitap Sayfamıza Hoşgeldiniz</h2>
       <br />
       <br />
       <table>
       @foreach (var k in Model)
       {
           <tr>
               <td>@k.ID</td>
               <td>@k.KitapAd</td>
               <td>@k.Yazar</td>
               <br/>
           </tr>
       }
       </table>
   
   </body>
</html>