@model CoreDepartment.Models.departments
@{
    ViewData["Title"] = "DepartmanGetir";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
@using(Html.BeginForm("DepartmanGüncelle","departmanlar",FormMethod.Post)){
   <b>Departman ID</b>
   @Html.TextBoxFor(x=>x.Id,new {@class="form-control"})
   <br />
   <b>Departman Adı</b>
   @Html.TextBoxFor(x=>x.departmanAd,new {@class="form-control"})
   <br />
   <button class="btn btn-info">Departman Güncelle</button>
}

